{
"name": "GPGmail",
"version": "1.0",
"background": {
    "scripts": [
	"lib/gmail.js",
	"lib/jquery-2.1.1.min.js"
	],
    "persistent": true
  },
"web_accessible_resources": [
	"lib/gmail.js",
	"lib/jquery-2.1.1.min.js"
  ],
"content_scripts": [
	{
	"matches": ["*://*.mail.google.com/*"],
	"js": ["inject.js"]
}
],
"manifest_version": 2
}